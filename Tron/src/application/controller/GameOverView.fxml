<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.LastViewController">
   <center>
      <Pane prefHeight="480.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="538.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../Images/Metal_Gear_Solid_(PSX)_20.png" />
               </image>
            </ImageView>
            <Label fx:id="label" layoutX="255.0" layoutY="75.0" prefHeight="90.0" prefWidth="245.0" textFill="#0ca7da">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <Button fx:id="playAgain" layoutX="149.0" layoutY="264.0" mnemonicParsing="false" onAction="#handlePlayAgain" text="Race Again! " />
            <Button fx:id="exit2Menu" layoutX="401.0" layoutY="264.0" mnemonicParsing="false" onAction="#handleGo2Menu" text="Go to Main Menu" />
         </children>
      </Pane>
   </center>
</BorderPane>
